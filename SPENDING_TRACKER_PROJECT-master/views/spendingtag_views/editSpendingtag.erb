<h2>Tags</h2>

<form method="post" action="/spendingtags/<%=@spendingtag.id%>">
  <label for="type-field"> Type:</label>
  <input
  type="text"
  name="type"
  id="type-field"
  value="<%=@spendingtag.type%>"
  />

  <input type="submit" value="Edit">
</form>

<form method = "post" action = "/spendingtags/<%=@spendingtag.id%>/delete">
  <input type="submit" value = "Delete" >
</form>
